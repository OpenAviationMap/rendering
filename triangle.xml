<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map bgcolor="white" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">

    <Style name="OpenAviationMap">
        <Rule>
            <LinePatternSymbolizer width="20" height="20" type="png"
                                   file="danger_area_edge.png"
            />
        </Rule>
    </Style>


    <Layer name="triangle" status="on" srs="+proj=latlong +datum=WGS84">
        <StyleName>OpenAviationMap</StyleName>
        <Datasource>
            <Parameter name="type">osm</Parameter>
            <Parameter name="file">triangle.osm</Parameter>
        </Datasource>
    </Layer>

</Map>

